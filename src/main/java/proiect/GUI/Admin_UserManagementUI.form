      <Properties>
        <Property name="text" type="java.lang.String" value="Find user"/>
      </Properties>
      <AuxValues>
        <AuxValue name="JavaCodeGenerator_AddingCodePost" type="java.lang.String" value="findUserButton.addActionListener(new ActionListener() {&#xa;    @Override&#xa;    public void actionPerformed(ActionEvent e) {&#xa;        // Determine the search type&#xa;        String searchType;&#xa;        if (jRadioButton1.isSelected()) {&#xa;            searchType = &quot;email&quot;;&#xa;        } else if (jRadioButton2.isSelected()) {&#xa;            searchType = &quot;phone&quot;;&#xa;        } else {&#xa;            searchType = &quot;id&quot;;&#xa;        }&#xa;&#xa;        // Retrieve the search term&#xa;        String searchTerm;&#xa;        if (searchType.equals(&quot;email&quot;)) {&#xa;            searchTerm = searchEmailField.getText();&#xa;        } else if (searchType.equals(&quot;phone&quot;)) {&#xa;            searchTerm = searchPhoneNumberField.getText();&#xa;        } else {&#xa;            searchTerm = searchUserIDField.getText();&#xa;        }&#xa;&#xa;        // Construct the endpoint URL&#xa;        String endpoint = &quot;https://example.com/api/users/&quot; + searchType + &quot;/&quot; + searchTerm;&#xa;&#xa;        // Make the HTTP request&#xa;        HttpClient client = HttpClient.newHttpClient();&#xa;        HttpRequest request = HttpRequest.newBuilder()&#xa;                .uri(URI.create(endpoint))&#xa;                .GET()&#xa;                .build();&#xa;        try {&#xa;            HttpResponse&lt;String&gt; response = client.send(request, HttpResponse.BodyHandlers.ofString());&#xa;&#xa;            if (response.statusCode() == 200) {&#xa;                // Parse the response body&#xa;                ObjectMapper mapper = new ObjectMapper();&#xa;                User user = mapper.readValue(response.body(), User.class);&#xa;&#xa;                // Populate the UI fields&#xa;                emailField.setText(user.getEmail());&#xa;                firstNameField.setText(user.getFirstName());&#xa;                lastNameField.setText(user.getLastName());&#xa;                phoneNumberField.setText(user.getPhoneNumber());&#xa;            } else {&#xa;                JOptionPane.showMessageDialog(null, &quot;User not found&quot;);&#xa;            }&#xa;        } catch (IOException | InterruptedException ex) {&#xa;            JOptionPane.showMessageDialog(null, &quot;Error retrieving user data&quot;);&#xa;        }&#xa;    }&#xa;});"/>
      </AuxValues>
    </Component>
